{
    "name": "boss24/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "boss24",
        "boss24 cms",
        "boss24 platform",
        "boss24 assets"
    ],
    "homepage": "https://boss24.ua",
    "authors": [
        {
            "name": "Sokol Fx",
            "email": "office@boss24.ua"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Boss24\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boss24\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Boss24\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Boss24\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
